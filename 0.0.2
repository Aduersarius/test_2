#include <iostream>
#include <sstream>
using namespace std;

int main() {

    int a,k; string line, o, u;

    getline(cin, o);

    istringstream stream(o);
    if (!(stream >> a)) {

        cout << "An error has occuried while reading input data.";

        return 0;
    }

    getline(cin, line);

    istringstream arr(line);

    int *m = new int[a];

    for (int i = 0; i < a; i++) {

        if (!(arr >> m[i])) {

            cout << "An error has occuried while reading input data.";

            return 0;
        }
    }
    
    getline(cin, u);
    istringstream stream1(u);
    if (!(stream1 >> k)) {

        cout << "An error has occuried while reading input data.";

        return 0;
    }
    
    int l = 0;
    for (int y = k;y < a/2+k-1; y++) {
        swap(m[y],m[a-l-1]);l++;
    }
    l = 0;
    for (int p = 0; p < k/2; p++){
        swap(m[p],m[k-l-1]);l++;
    }
    for (int i = 0; i < a/2 ; i++) {
        swap(m[i],m[a-i-1]);
    }
    for (int i = 0; i < a; i++) { cout << m[i] << ' '; }


    return 0;

}
